<meta property="og:image"               content="<%= object.file.url %>"/>
<meta property="og:image:secure_url"    content="<%= object.file.url.replace('http','https') %>" />
<meta property="og:image:alt"           content="<%= object.alt %>"/>
<meta property="og:image:type"          content="<%= object.mime_type %>"/>
<meta property="og:image:width"         content="<%= object.width %>"/>
<meta property="og:image:height"        content="<%= object.height %>"/>