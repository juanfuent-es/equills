<meta property="og:audio" content="<%= object.file.url %>"/>
<meta property="og:audio:secure_url" content="<%= object.file.url.replace('http','https') %>"/>
<meta property="og:audio:type" content="audio/<%= object.mime_type %>"/>
<meta property="og:video" content="<%= object.file.url %>"/>
<meta property="og:video:secure_url" content="<%= object.file.url.replace('http','https') %>"/>
<meta property="og:video:type" content="<%= object.mime_type %>"/>
<meta property="og:video:width" content="<%= object.width %>"/>
<meta property="og:video:height" content="<%= object.height %>"/>